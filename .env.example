# DeFi Agent Environment Configuration
# Copy this file to .env and fill in your values

# Required: Your private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Optional: Custom Base RPC URL (defaults to mainnet.base.org if not set)
BASE_RPC_URL=https://mainnet.base.org

# Optional: Specific gauge address (will be auto-discovered if not provided)
GAUGE_ADDRESS=
